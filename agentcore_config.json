{
  "agentcore": {
    "version": "1.0.0",
    "service_name": "PropertyPilot-Gemini",
    "description": "AI-powered real estate investment analysis system with Google Gemini 2.5 Pro",
    "capabilities": {
      "built_in_tools": {
        "enabled": true,
        "tools": [
          "zillow_property_search",
          "market_analysis",
          "roi_calculator",
          "demographic_analyzer",
          "risk_assessor",
          "web_research"
        ]
      },
      "gateways": {
        "enabled": true,
        "api_gateway": {
          "rate_limiting": true,
          "authentication": "cognito",
          "cors_enabled": true
        },
        "websocket_gateway": {
          "enabled": true,
          "real_time_updates": true
        }
      },
      "memory": {
        "enabled": true,
        "strategies": [
          "summaryMemoryStrategy",
          "userPreferenceMemoryStrategy",
          "semanticMemoryStrategy"
        ],
        "namespaces": [
          "/summaries/{actorId}/{sessionId}",
          "/preferences/{actorId}",
          "/facts/{actorId}",
          "/properties/{actorId}",
          "/market_data/{actorId}"
        ],
        "retention_policy": {
          "summary_retention_days": 90,
          "preference_retention_days": 365,
          "fact_retention_days": 180
        }
      },
      "observability": {
        "enabled": true,
        "cloudwatch_logs": true,
        "xray_tracing": true,
        "custom_metrics": true,
        "prometheus_metrics": true,
        "dashboards": {
          "performance_dashboard": true,
          "business_metrics_dashboard": true,
          "error_tracking_dashboard": true
        },
        "alerts": {
          "high_error_rate": {
            "threshold": 5,
            "period_minutes": 5
          },
          "high_latency": {
            "threshold_ms": 10000,
            "period_minutes": 5
          },
          "low_success_rate": {
            "threshold_percent": 95,
            "period_minutes": 10
          }
        }
      },
      "identity": {
        "enabled": true,
        "provider": "cognito",
        "features": {
          "user_pools": true,
          "identity_pools": true,
          "mfa": true,
          "social_login": false,
          "custom_attributes": [
            "investment_preferences",
            "risk_tolerance",
            "investment_timeline",
            "preferred_locations",
            "max_budget"
          ]
        },
        "session_management": {
          "session_timeout_minutes": 45,
          "max_sessions_per_user": 15,
          "session_persistence": true
        }
      }
    },
    "runtime_configuration": {
      "memory_size_mb": 4096,
      "timeout_seconds": 900,
      "concurrent_executions": 100,
      "environment_variables": {
        "MODEL_PROVIDER": "gemini",
        "GEMINI_MODEL_ID": "gemini-2.5-pro",
        "LOG_LEVEL": "INFO",
        "ENABLE_AGENTCORE_OBSERVABILITY": "true",
        "AGENTCORE_MEMORY_ENABLED": "true",
        "SESSION_PERSISTENCE_ENABLED": "true",
        "AUTO_SCALING_ENABLED": "true"
      }
    },
    "networking": {
      "mode": "PUBLIC",
      "vpc_configuration": {
        "security_groups": [],
        "subnets": []
      }
    },
    "security": {
      "encryption_at_rest": true,
      "encryption_in_transit": true,
      "iam_role_permissions": [
        "bedrock-agentcore:InvokeAgentRuntime",
        "bedrock-agentcore:CreateEvent",
        "bedrock-agentcore:GetEvent",
        "bedrock-agentcore:ListEvents",
        "cognito-idp:AdminGetUser",
        "cognito-idp:AdminInitiateAuth",
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents",
        "xray:PutTraceSegments",
        "xray:PutTelemetryRecords",
        "cloudwatch:PutMetricData"
      ]
    }
  },
  "model_configuration": {
    "provider": "google_gemini",
    "model_id": "gemini-2.5-pro",
    "parameters": {
      "temperature": 0.3,
      "max_output_tokens": 4096,
      "top_p": 0.9,
      "top_k": 40
    },
    "features": {
      "thinking_enabled": true,
      "google_search_enabled": true,
      "multimodal_support": true,
      "structured_output": true
    }
  },
  "business_logic": {
    "real_estate_analysis": {
      "supported_locations": ["US", "Canada"],
      "property_types": [
        "single_family",
        "multi_family",
        "condo",
        "townhouse",
        "commercial"
      ],
      "analysis_types": [
        "market_research",
        "property_valuation",
        "roi_analysis",
        "risk_assessment",
        "comparative_analysis"
      ]
    },
    "data_sources": {
      "zillow_api": {
        "enabled": true,
        "provider": "hasdata"
      },
      "demographic_data": {
        "enabled": true,
        "source": "public_estimates"
      },
      "market_trends": {
        "enabled": true,
        "source": "economic_indicators"
      }
    }
  }
}